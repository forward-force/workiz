{
    "name": "forward-force/workiz",
    "description": "PHP Wrapper around Workiz API",
    "type": "library",
    "keywords": [
        "forwardforce",
        "workiz"
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^7.1@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5@dev",
        "squizlabs/php_codesniffer": "^3.0@dev",
        "phpmd/phpmd": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "ForwardForce\\Workiz\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ForwardForce\\Workiz\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Teodor Talov",
            "email": "teodor@forwardforce.io"
        }
    ],
    "minimum-stability": "dev"
}
